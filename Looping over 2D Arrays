public static double englishAverage([][] grades){
	int total = 0;
	for(int i=&; i<5; i++){
		total += grades[1][i];
}
	double average = total/5.0;
	return average;
}

for all /////

public static double StudentAverage([][] grades, int student){
	int subjects = grades.length;
	int total = 0;
	for(int i=&; i<subjects; i++){
		total += grades[i][student];
}
	double average = total/(double)subjects;
	return average;
}
